{
  "id": 15,
  "title": "Number of thefts Vs year for the period 2009-2012",
  "data": {
    "url": "homicides3.csv"
  },
  "transform": [{
    "filter": {
      "field": "Primary Type",
      "equal": "THEFT"
    }
  }],
  "mark": {
    "type": "line",
    "interpolate": "monotone"
  },
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "YEAR",
      "type": "temporal",
      "axis": {
        "title": "YEAR"
      }
    },
    "y": {
      "aggregate": "count",
      "field": "*",
      "type": "quantitative"
    }
  },
  "color": {
    "field": "Description",
    "type": "nominal"
  },
  "config": {
    "axis": {
      "shortTimeLabels": true
    }
  },
  "user": "Deepika",
  "chartType": "line",
  "dateTime": {
    "fullTime": "Sept 10 2018",
    "year": "2018",
    "month": "09",
    "date": "10"
  },
  "description": "This dataset reveals that there are maximum number of thefts in 2008 around 100,000 and then to around 80,000 in 2009 and drastically decreased from 2009 to 2011."
}
